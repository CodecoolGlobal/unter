       direction=\"horizontal\"\n                />\n            </MediaQuery>\n\n            <MediaQuery minDeviceWidth={415}>\n                <DateRange\n                    ranges={[selectionRange]}\n                    onChange={handleSelect}\n                    months={2}\n                    rangeColors={[\"rgb(240,242,247)\"]}\n                    moveRangeOnFirstSelection={false}\n                    editableDateInputs={true}\n                    direction=\"horizontal\"/>\n            </MediaQuery>\n        </div>\n            )\n        }\n       \n\n\nexport default Search;\n","/home/mark/ADVANCED/unter/frontend/src/search_page/SearchPage.jsx",["82"],"/home/mark/ADVANCED/unter/frontend/src/App.js",["83"],"import \"./App.scss\";\nimport React, { useState } from \"react\";\nimport Home from \"./home_page/Home\";\nimport Header from \"./permanent/Header\";\nimport Footer from \"./permanent/Footer\";\nimport { Route, BrowserRouter as Router, Switch } from \"react-router-dom\";\nimport SearchPage from \"./search_page/SearchPage\";\nimport { HeaderProvider } from \"./context/HeaderCloseContext\";\nimport AccommodationPage from \"./accommodation_page/AccommodationPage\";\n\nfunction App() {\n    return (\n        <div className=\"App\">\n            <Router>\n                <HeaderProvider>\n                    <Header />\n                    <Switch>\n                        <Route path=\"/search\">\n                            <SearchPage />\n                        </Route>\n                        <Route path=\"/accommodation\">\n                            <AccommodationPage/>\n                        </Route>\n                        <Route path=\"/\">\n                            <Home />\n                        </Route>\n                    </Switch>\n                </HeaderProvider>\n                <Footer />\n            </Router>\n        </div>\n    );\n}\n\nexport default App;\n","/home/mark/ADVANCED/unter/frontend/src/context/HeaderCloseContext.jsx",[],["84","85"],"/home/mark/ADVANCED/unter/frontend/src/index.js",[],["86","87"],"/home/mark/ADVANCED/unter/frontend/src/permanent/Header.jsx",["88","89"],"/home/mark/ADVANCED/unter/frontend/src/search_page/SearchResult.jsx",[],["90","91"],"/home/mark/ADVANCED/unter/frontend/src/permanent/SearchBar.jsx",[],"/home/mark/ADVANCED/unter/frontend/src/accommodation_page/AccommodationPage.jsx",[],"/home/mark/ADVANCED/unter/frontend/src/accommodation_page/AccommodationCalendar.jsx",["92"],"import React ,{useState}from 'react'\nimport './AccommodationCalendar.scss'\nimport { DateRange } from \"react-date-range\";\nimport MediaQuery from 'react-responsive'\n\nfunction AccommodationCalendar() {\n    const [startDate, setStartDate] = useState(new Date());\n    const [endDate, setEndDate] = useState(new Date());\n\n\n    const selectionRange = {\n        startDate: startDate,\n        endDate: endDate,\n        key: \"selection\",\n    };\n\n    function handleSelect(ranges) {\n        setStartDate(ranges.selection.startDate);\n        setEndDate(ranges.selection.endDate);\n    }\n    return (\n        <div className=\"calendar\">\n                <DateRange\n                    ranges={[selectionRange]}\n                    onChange={handleSelect}\n                    months={2}\n                    rangeColors={[\"rgb(240,242,247)\"]}\n                    moveRangeOnFirstSelection={false}\n                    editableDateInputs={true}\n                    direction=\"horizontal\"\n                />\n        </div>\n    )\n}\n\nexport default AccommodationCalendar;\n","/home/mark/ADVANCED/unter/frontend/src/home_page/Card.jsx",[],"/home/mark/ADVANCED/unter/frontend/src/permanent/Login.jsx",["93"],"/home/mark/ADVANCED/unter/frontend/src/permanent/Dropdown.jsx",["94"],"/home/mark/ADVANCED/unter/frontend/src/permanent/Registration.jsx",["95"],"import React,{useState} from 'react'\nimport Modal from '@material-ui/core/Modal';\nimport './Registration.scss';\nimport CloseIcon from '@material-ui/icons/Close';\n\nfunction Registration() {\n    const [open, setOpen] = useState(false);\n  const [firstNameEffect,setFirstNameEffect] = useState(false);\n  const [lastNameEffect,setLastNameEffect] = useState(false);\n  const [birthDateEffect,setBirthDateEffect] = useState(false);\n  const [emailEffect,setEmailEffect] = useState(false);\n  const [pswEffect,setPswEffect] = useState(false);\n  const handleOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  function handleTextChange(target) {\n      switch(target.id){\n        case \"firstName\":\n            target.value=== ''? setFirstNameEffect(false): setFirstNameEffect(true);\n            break;\n        case \"lastName\":\n            target.value=== ''? setLastNameEffect(false): setLastNameEffect(true);\n            break;\n        case \"birthDate\":\n            target.value=== ''? setBirthDateEffect(false): setBirthDateEffect(true);\n            break;\n        case \"email\":\n            target.value=== ''? setEmailEffect(false): setEmailEffect(true);\n            break;\n        case \"psw\":\n            target.value=== ''? setPswEffect(false): setPswEffect(true);\n            break;\n      }\n     \n\n  }\n    const body = (\n        <div className=\"register__modal\">\n            <div className=\"register__header\">\n                <CloseIcon onClick={handleClose}/>\n                <h2 className=\"title\">Registration</h2>\n            </div>\n            <div className=\"register__body\">\n                <div className=\"nameArea\">\n                    <div className=\"label\">\n                        <input type=\"text\" id=\"firstName\"  onChange={(e) => handleTextChange(e.target)}/>\n                        <label className={ firstNameEffect ? \"Active\" : \"\"} htmlFor=\"firstName\">First name</label>\n                    </div>\n                    <div className=\"label\">\n                        <input type=\"email\" id=\"lastName\"  onChange={(e) => handleTextChange(e.target)}/>\n                        <label className={ lastNameEffect ? \"Active\" : \"\"} htmlFor=\"lastName\">Last Name</label>\n                    </div>\n                </div>\n                    <h5>Gondoskodj róla, hogy egyezzen a hivatalos személyazonosító okmányodon szereplő névvel.\n</h5>\n                <div className=\"birthArea\">\n                    <div className=\"label\">\n                        <input type=\"email\" id=\"birthDate\"  onChange={(e) => handleTextChange(e.target)}/>\n                        <label className={ birthDateEffect ? \"Active\" : \"\"} htmlFor=\"birthDate\">Bith date</label>\n                    </div>\n                </div>\n                <h5>A regisztrációhoz legalább 18 évesnek kell lenned. A születésnapodat nem osztjuk meg az Airbnb más felhasználóival.</h5>\n                <div className=\"emailArea\">\n                    <div className=\"label\">\n                        <input type=\"email\" id=\"email\"  onChange={(e) => handleTextChange(e.target)}/>\n                        <label className={ emailEffect ? \"Active\" : \"\"} htmlFor=\"email\">Email</label>\n                    </div>\n                </div>\n                <h5>Küldünk majd egy e-mailt az utazás visszaigazolásaival és nyugtáival.\n</h5>\n                <div className=\"pswArea\">\n                    <div className=\"label\">\n                        <input type=\"psw\" id=\"psw\"  onChange={(e) => handleTextChange(e.target)}/>\n                        <label  className={ pswEffect ? \"Active\" : \"\"} htmlFor=\"psw\">Password</label>\n                    </div>\n                </div>\n                <h5>A lenti Elfogadás és folytatás gombra kattintva elfogadom az Airbnb által meghatározott Általános Szerződési Feltételek, Fizetésekre vonatkozó szerződési feltételek, Adatvédelmi Szabályzat és Diszkriminációellenes szabályzat rendelkezéseit.</h5>\n            </div>\n            <button className=\"register__button\">Register</button>\n            <h6>Az Airbnb csak tagoknak szóló ajánlatokat, inspirációt, marketing-e-maileket és push-értesítéseket fog küldeni neked. A fiókbeállításoknál vagy közvetlenül a marketingértesítésben is bármikor leállíthatod az ilyen típusú üzenetek fogadását.\n\n</h6>\n        </div>\n      );\n\n\n    return (\n    <div className=\"registration__container\">\n        <p onClick={handleOpen}>Register</p>\n        <Modal\n            open={open}\n            onClose={handleClose}\n            aria-labelledby=\"simple-modal-title\"\n            aria-describedby=\"simple-modal-description\"\n        >\n        {body}\n        </Modal>\n    </div>\n    )\n}\n\nexport default Registration\n","/home/mark/ADVANCED/unter/frontend/src/permanent/Footer.jsx",[],["96","97"],"/home/mark/ADVANCED/unter/frontend/src/home_page/Banner.jsx",[],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"message":"103","line":5,"column":21,"nodeType":"104","messageId":"105","endLine":5,"endColumn":36},{"ruleId":"102","severity":1,"message":"106","line":16,"column":12,"nodeType":"104","messageId":"105","endLine":16,"endColumn":21},{"ruleId":"102","severity":1,"message":"107","line":2,"column":17,"nodeType":"104","messageId":"105","endLine":2,"endColumn":25},{"ruleId":"98","replacedBy":"108"},{"ruleId":"100","replacedBy":"109"},{"ruleId":"98","replacedBy":"110"},{"ruleId":"100","replacedBy":"111"},{"ruleId":"102","severity":1,"message":"112","line":5,"column":10,"nodeType":"104","messageId":"105","endLine":5,"endColumn":16},{"ruleId":"102","severity":1,"message":"113","line":8,"column":8,"nodeType":"104","messageId":"105","endLine":8,"endColumn":13},{"ruleId":"98","replacedBy":"114"},{"ruleId":"100","replacedBy":"115"},{"ruleId":"102","severity":1,"message":"116","line":4,"column":8,"nodeType":"104","messageId":"105","endLine":4,"endColumn":18},{"ruleId":"117","severity":1,"message":"118","line":12